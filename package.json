{
  "name": "lyrics-finder",
  "version": "4.0.0",
  "description": "Lyrics Finder bot on Discord",
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": "https://github.com/angeloanan/lyrics-finder",
  "author": "Christopher Angelo <angelo@angeloanan.xyz>",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@types/pino": "^7.0.5",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "discord-api-types": "^0.30.0",
    "eslint": "^8.12.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-node": "11",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.1",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "type-fest": "^2.12.1"
  },
  "dependencies": {
    "@types/common-tags": "^1.8.1",
    "bufferutil": "^4.0.6",
    "common-tags": "^1.8.2",
    "discord.js": "^13.6.0",
    "dotenv": "^16.0.0",
    "execall": "^3.0.0",
    "node-fetch": "^3.2.3",
    "p-retry": "^5.1.0",
    "pino": "^7.9.2",
    "pretty-ms": "^7.0.0",
    "typescript": "^4.6.3",
    "utf-8-validate": "^5.0.9",
    "zlib-sync": "^0.1.7"
  },
  "scripts": {
    "test": "jest --coverage",
    "build": "tsc",
    "start": "node ./target/index.js",
    "clean": "rm -r node_modules && rm -r target",
    "dev": "nodemon --delay 5 -e ts,json --exec \"node --experimental-specifier-resolution=node --trace-warnings --loader ts-node/esm src/index.ts\"",
    "profiler": "tsc && node ./target/index.js --harmony --prof",
    "format": "eslint --fix && prettier --write .",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
